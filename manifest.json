{
  "name": "Chime",
  "short_name": "Chime",
  "version": "0.1.1",
  "description": "Speak time at 15min interval",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["alarms", "storage"],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "manifest_version": 2
}
