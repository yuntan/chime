{
  "name": "Chime",
  "version": "1.0",
  "description": "Speak time at 15min interval",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["alarms", "storage"],
  "options_page": "options.html",
  "icons": {
    "16": "icon-16x16.png",
    "32": "icon-32x32.png",
    "48": "icon-48x48.png",
    "128": "icon-128x128.png"
  },
  "manifest_version": 2
}
